model ChatbotContact {
  id        Int      @id @default(autoincrement())
  nom       String
  prenom    String
  email     String
  telephone String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 
model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  name     String?
}